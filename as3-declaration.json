{
  "class": "AS3",
  "action": "deploy",
  "persist": true,
  "declaration": {
    "class": "ADC",
    "schemaVersion": "3.0.0",
    "service1": {
      "class": "Tenant",
      "web_app1": {
        "class": "Application",
        "template": "https",
        "serviceMain": {
          "class": "Service_HTTPS",
          "virtualAddresses": [
            { "use": "serviceAddress" }
          ],
          "virtualPort": 443,
          "pool": "web_app1_web_pool",
          "policyWAF": {
            "use": "web_app1_waf_policy"
          },
          "securityLogProfiles": [ 
            {
              "use": "secLogRemote"
            },
            {
              "bigip": "/Common/Log illegal requests"
            }
          ],
          "serverTLS": "web_app1_client_ssl",
          "profileHTTP": {
            "use": "http_profile_xff"
          }
        },
        "serviceAddress":{
          "class":"Service_Address",
          "virtualAddress":"10.0.0.0/8",
          "arpEnabled":true,
          "icmpEcho":"enable",
          "routeAdvertisement":"enable",
          "spanningEnabled":false,
          "trafficGroup":"default"
        },
        "web_app1_web_pool": {
          "class": "Pool",
          "loadBalancingMode": "predictive-node",
          "monitors": [
            "http"
          ],
          "members": [
            {
              "servicePort": 8443,
              "addressDiscovery": "fqdn",
              "autoPopulate": true,
              "queryInterval": 10,
              "hostname": "aws-demo-svr-1a.silliw-nhoj.com"
            }
          ]
        },
        "web_app1_client_ssl": {
          "class": "TLS_Server",
          "certificates": [
            {
              "certificate": "web_app1_cert"
            }
          ]
        },
        "http_profile_xff": {
          "class": "HTTP_Profile",
          "xForwardedFor": true
        },
        "web_app1_cert": {
          "class": "Certificate",
          "remark": "in practice we recommend using a passphrase",
          "certificate": "-----BEGIN CERTIFICATE-----\\n-----END CERTIFICATE-----",
          "privateKey": "-----BEGIN RSA PRIVATE KEY-----\\n-----END RSA PRIVATE KEY-----"
        },
        "web_app1_waf_policy": {
          "class": "WAF_Policy",
          "url": "https://github.com/f5devcentral/f5-asm-policy-templates/raw/master/owasp_ready_template/owasp-auto-tune-v1.1.xml",
          "ignoreChanges": true
        },
        "secLogRemote": {
          "class": "Security_Log_Profile",
          "application": {
            "localStorage": false,
            "maxEntryLength": "2k",
            "protocol": "tcp",
            "remoteStorage": "bigiq",
            "reportAnomaliesEnabled": true,
            "servers": [
              {
                "address": "10.1.10.6",
                "port": "8514"
              }
            ]
          }
        }
      }
    }
  }
}